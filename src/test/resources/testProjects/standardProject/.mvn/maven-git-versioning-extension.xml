<configuration>
    <branch>
        <pattern>master</pattern>
        <versionFormat>${version.release}</versionFormat>
        <property>
            <name>foo</name>
            <valueFormat>new_new_new</valueFormat>
        </property>
    </branch>
    <branch>
        <pattern>feature/(.*)</pattern>
        <versionFormat>${1}-SNAPSHOT</versionFormat>
    </branch>
    <tag>
        <pattern>v([0-9].*)</pattern>
        <versionFormat>${1}</versionFormat>
    </tag>
</configuration>
