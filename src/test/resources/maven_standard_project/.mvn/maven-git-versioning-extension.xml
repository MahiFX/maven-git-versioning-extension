<configuration>
    <branches>
        <branch>
            <pattern>develop</pattern>
            <versionFormat>${version.release}</versionFormat>
        </branch>
        <branch>
            <pattern>master</pattern>
            <versionFormat>${version.release}</versionFormat>
        </branch>
        <branch>
            <pattern>GHFH_23423_tests_VB</pattern>
            <versionFormat>${version.release}-custom_branch</versionFormat>
        </branch>
        <branch>
            <pattern>release/.*</pattern>
            <prefix>release/</prefix>
            <versionFormat>${branch}-SNAPSHOT</versionFormat>
        </branch>
        <branch>
            <pattern><![CDATA[feature/(?<feature>.*)]]></pattern>
            <versionFormat>${feature}-SNAPSHOT</versionFormat>
        </branch>
    </branches>
    <tags>
        <tag>
            <pattern>.*</pattern>
            <prefix>version/</prefix>
            <versionFormat>${tag}</versionFormat>
        </tag>
    </tags>
</configuration>
